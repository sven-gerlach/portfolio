{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Portfolio website for Sven Gerlach",
  "private": true,
  "scripts": {
    "build": "webpack --config config/webpack.prod.js",
    "serve": "webpack serve --config config/webpack.dev.js",
    "prepare": "husky install",
    "prettier:check": "prettier --check --ignore-unknown --no-error-on-unmatched-pattern src aws-cdk",
    "prettier:write": "prettier --write --ignore-unknown --no-error-on-unmatched-pattern src aws-cdk",
    "eslint:check": "npx eslint --no-error-on-unmatched-pattern src",
    "eslint:write": "npx eslint --no-error-on-unmatched-pattern --fix src"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sven-gerlach/portfolio.git"
  },
  "author": "Sven Gerlach",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sven-gerlach/portfolio/issues"
  },
  "homepage": "https://github.com/sven-gerlach/portfolio#readme",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/lodash": "^4.14.188",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
